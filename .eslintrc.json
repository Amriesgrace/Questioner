{
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": 6
    },
    "env": {
        "browser": true,
        "node": true
    },
    "rules": {
        "indent": ["error", 4],
        "comma-dangle":"off",
        "linebreak-style": ["error", "unix"],
        "no-multiple-empty-lines": ["error", { "max": 4, "maxEOF": 1 }]
    }
}